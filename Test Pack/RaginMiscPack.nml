//EMD_GF6CH_Liv0
spriteset(spriteset_EMD_GF6CH_Liv0_Purchase,   "gfx/EMD_GF6CH_Liv0.png") {template_purchase(7, 21)}
spriteset(spriteset_EMD_GF6CH_Liv0,   "gfx/EMD_GF6CH_Liv0.png") {template_8(7, 39)}
spriteset(spriteset_EMD_GF6CH_Liv0_r,   "gfx/EMD_GF6CH_Liv0.png") {template_8_r(7, 39)}

switch(FEAT_TRAINS, SELF, switch_EMD_GF6CH_Liv0_Fw_railtype, tile_powers_railtype("ELRL")) {
 0: spriteset_EMD_GF6CH_PantoDown;
 1: spriteset_EMD_GF6CH_PantoUp;
}

switch (FEAT_TRAINS, SELF, switch_EMD_GF6CH_Liv0_Fw, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
    getbits(extra_callback_info1, 8, 8)
    ]) {
    0: spriteset_EMD_GF6CH_Liv0;
    1: switch_EMD_GF6CH_Liv0_Fw_railtype;
}

switch(FEAT_TRAINS, SELF, switch_EMD_GF6CH_Liv0_Fwr_railtype, tile_powers_railtype("ELRL")) {
 0: spriteset_EMD_GF6CH_PantoDown;
 1: spriteset_EMD_GF6CH_PantoUp_Fwr;
}

switch (FEAT_TRAINS, SELF, switch_EMD_GF6CH_Liv0_Fwr, [
    STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100),
    getbits(extra_callback_info1, 8, 8)
    ]) {
    0: spriteset_EMD_GF6CH_Liv0;
    1: switch_EMD_GF6CH_Liv0_Fwr_railtype;
}

switch(FEAT_TRAINS, SELF, switch_EMD_GF6CH_Liv0_Fwflipcheck, vehicle_is_flipped) {
 0: switch_EMD_GF6CH_Liv0_Fw;
